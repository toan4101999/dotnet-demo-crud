@model Job;
@{
    ViewData["Title"] = "Edit Job";
    Job job = Model;
}
<form action="/Admin/SaveUpdateJob" method="post">
    <div class="form-group">
        <label for="Id">JobId</label>
        <input type="number" class="form-control" id="JobId" name="JobId" value="@job.JobId">
    </div>
    <div class="form-group">
        <label for="NameJob">NameJob</label>
        <input type="text" class="form-control" id="NameJob" name="NameJob" value="@job.nameJob">
    </div>
    <div class="form-group">
        <label for="NameCompany">NameCompany</label>
        <input type="text" class="form-control" id="NameCompany" name="NameCompany" value="@job.nameCompany">
    </div>
    <div class="form-group">
        <label for="Salary">Salary</label>
        <input type="text" class="form-control" id="Salary" name="Salary" value="@job.salary">
    </div>
    <div class="form-group">
        <label for="Location">Location</label>
        <input type="text" class="form-control" id="Location" name="Location" value="@job.location">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>